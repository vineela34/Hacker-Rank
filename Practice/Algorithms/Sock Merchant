#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>
int main(){
    int n,x;
    scanf("%d",&n);
    int a[101] = {0};
    if(n>=1 && n<=100){
    for(int i=0;i<n;i++){
        scanf("%d",&x);
        a[x]++;
    }
    int max=0;
    for(int i=0;i<101;i++){
         max += a[i]/2;
    }
    
    printf("%d",max);
        return 0;
    }
}
