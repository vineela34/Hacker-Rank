using namespace std;
void rev(int a[],int n){
    int ar[n],j=0;
    for(int i=n-1;i>=0;i--){
        ar[j]=a[i];
        j++;
    }
    for(int i=0;i<n;i++)
        cout<<ar[i]<<" ";
    cout<<endl;
}

int main(){
    int n;
    cin >> n;
    int arr[n];
    for(int arr_i = 0;arr_i < n;arr_i++){
       cin >> arr[arr_i];
    }
    rev(arr,n);
    return 0;
}
